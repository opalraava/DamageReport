{
    "manifest_version": 2,
    
    "name": "DamageReport",
    "short_name": "DamageReport",
    "description": "This gives you a damage report.",
    "version": "0.1.1",
    
    "icons": { "128": "dmg_report-128.png" },
    
    "permissions": [ "tabs", "activeTab", "storage" ],

    "browser_action": {
		"default_icon": "dmg_report-512.png",
		"default_title": "DamageReport",
		"default_popup": "popup.html"
    }
}
